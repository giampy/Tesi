type mytype[large, fixed].
fun G(mytype): mytype.
fun G'(mytype): mytype.
param n1, n2.
query secret1 w.
proba POW.
equiv
	foreach i1<=n1 do
            r <-R mytype;
            OGet():=return (G'(r))
     
  	 <=(0)=> (*da definire*)
	
	foreach i1 <=n1 do
             w <-R mytype;
             OGet():=return(w).


forall x:mytype; G(x)=(G'(G'(x))).

let processO=
	foreach i1 <=n2 do
		O():=
			r<-R mytype;
			w: mytype <-G(r);
		return.
process
		processO
